{
  "name": "ecsstractor",
  "displayName": "eCSStractor",
  "description": "Extracting selectors from HTML and generate CSS stylesheet",
  "version": "2.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kubosho/vscode-ecsstractor.git"
  },
  "publisher": "kubosho",
  "engines": {
    "vscode": "^1.28.0",
    "node": ">=10.0.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.run",
        "title": "Run: eCSStractor"
      }
    ]
  },
  "scripts": {
    "fmt": "prettier --write ./src/**/*.{js,ts}",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "ava"
  },
  "devDependencies": {
    "@types/htmlparser2": "3.10.1",
    "@types/node": "10.12.18",
    "@types/prettier": "1.18.2",
    "ava": "2.4.0",
    "eslint": "6.5.1",
    "ts-node": "8.3.0",
    "typescript": "3.5.3",
    "vscode": "1.1.36"
  },
  "dependencies": {
    "htmlparser2": "^4.0.0",
    "prettier": "^1.14.3"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "files": [
      "./src/__tests__/test_*.ts"
    ],
    "sources": [
      "**/*.{js}"
    ],
    "concurrency": 5,
    "failFast": false,
    "failWithoutAssertions": false,
    "tap": false,
    "powerAssert": true
  }
}
