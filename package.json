{
  "name": "ecsstractor",
  "displayName": "eCSStractor",
  "description": "Extracting selectors from HTML and generate CSS stylesheet",
  "version": "2.1.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kubosho/vscode-ecsstractor.git"
  },
  "publisher": "kubosho",
  "engines": {
    "vscode": "^1.46.0",
    "node": ">=14.4.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.ecsstractor.run"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.ecsstractor.run",
        "title": "Run: eCSStractor"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "test": "ava",
    "test_with_vscode": "node ./out/__tests__/runTest.js",
    "format": "prettier --write ./src/**/*.{js,ts}",
    "release": "semantic-release"
  },
  "devDependencies": {
    "@types/glob": "7.1.3",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.2.0",
    "@semantic-release/release-notes-generator": "9.0.1",
    "@types/htmlparser2": "3.10.2",
    "@types/mocha": "8.2.0",
    "@types/node": "13.13.12",
    "@types/prettier": "2.1.6",
    "@types/vscode": "1.52.0",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "ava": "3.9.0",
    "eslint": "7.4.0",
    "glob": "7.1.6",
    "mocha": "8.2.1",
    "typescript": "4.1.3",
    "semantic-release": "17.2.3",
    "semantic-release-vsce": "3.0.1",
    "vscode": "1.1.37",
    "vscode-test": "1.4.0"
  },
  "dependencies": {
    "htmlparser2": "^4.0.0",
    "option-t": "^22.4.1",
    "prettier": "^2.0.5"
  }
}
