{
  "name": "ecsstractor",
  "displayName": "eCSStractor",
  "description": "Extracting selectors from HTML and generate CSS stylesheet",
  "version": "2.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kubosho/vscode-ecsstractor.git"
  },
  "publisher": "kubosho",
  "engines": {
    "vscode": "^1.28.0",
    "node": ">=10.0.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.run",
        "title": "Run: eCSStractor"
      }
    ]
  },
  "scripts": {
    "fmt": "prettier --write ./src/**/*.js",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "ava"
  },
  "devDependencies": {
    "ava": "1.0.0-rc.2",
    "eslint": "5.9.0",
    "vscode": "1.1.26"
  },
  "dependencies": {
    "htmlparser2": "^3.9.2",
    "prettier": "^1.14.3"
  },
  "ava": {
    "files": [
      "**/*.test.js"
    ],
    "sources": [
      "**/*.{js}"
    ],
    "concurrency": 5,
    "failFast": false,
    "failWithoutAssertions": false,
    "tap": false,
    "powerAssert": true
  }
}
